  "dependencies": {
    "@nostream-ilp/lib-dassie-reactive": "workspace:^",
    "@nostream-ilp/lib-dassie-type-utils": "workspace:^",
    "uint8array-extras": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "bugs": {
    "url": "https://github.com/cameri/nostream/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cameri/nostream.git",
    "directory": "packages/lib-dassie-reactive-io"
  }
